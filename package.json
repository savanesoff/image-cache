{
  "name": "image-cache-pro",
  "private": false,
  "version": "0.5.0",
  "type": "module",
  "description": "JavaScript library for caching images in memory. Provides ways to manage memory usage (RAM & GPU).",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "license": "MIT",
  "author": {
    "name": "Samvel Avanesov",
    "email": "sam.avanesoff@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/savanesoff/image-cache"
  },
  "packageManager": "pnpm@9.1.4",
  "keywords": [
    "image cache",
    "image rendering",
    "asset management",
    "ram",
    "gpu",
    "memory",
    "video memory",
    "GPU memory",
    "memory management",
    "typescript"
  ],
  "release": {
    "branches": [
      "main"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "lint": "eslint . --ignore-pattern dist/",
    "format": "prettier --write .",
    "test": "vitest --run src/lib/network/",
    "prepack": "pnpm run build",
    "clean:package": "rm -rf ./dist",
    "build:types": "tspc --project tsconfig.package.json",
    "build:package": "pnpm run clean:package && vite build && pnpm run build:types",
    "release": "pnpm run build:package && pnpm run test && pnpm changeset publish",
    "build": "pnpm run build:package"
  },
  "dependencies": {
    "events": "^3.3.0",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.5",
    "@eslint/js": "^9.3.0",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/events": "^3.0.0",
    "@types/json-schema": "^7.0.11",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^9.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-require-extensions": "^0.1.3",
    "eslint-plugin-vitest-globals": "^1.5.0",
    "jsdom": "^24.0.0",
    "lorem-ipsum": "^2.0.8",
    "path": "^0.12.7",
    "prettier": "^3.2.5",
    "rollup": "^4.17.2",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.1.2",
    "typescript": "^5.4.5",
    "typescript-transform-paths": "^3.4.7",
    "vite": "^5.2.11",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.4.0"
  }
}
